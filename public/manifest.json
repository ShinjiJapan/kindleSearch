{
  "manifest_version": 3,
  "version": "1.991",
  "name": "Kindle Search",
  "description": "kindle検索",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://www.amazon.co.jp/"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}